{
	"name": "pull_bike_status",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "upsert",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['legacy_id']"
								},
								"sink": {
									"path": "$['id']"
								}
							},
							{
								"source": {
									"path": "['num_ebikes_available']"
								},
								"sink": {
									"path": "$['num_ebikes_available']"
								}
							},
							{
								"source": {
									"path": "['num_bikes_available']"
								},
								"sink": {
									"path": "$['num_bikes_available']"
								}
							},
							{
								"source": {
									"path": "['last_reported']"
								},
								"sink": {
									"path": "$['last_reported']"
								}
							},
							{
								"source": {
									"path": "['num_docks_available']"
								},
								"sink": {
									"path": "$['num_docks_available']"
								}
							},
							{
								"source": {
									"path": "['station_id']"
								},
								"sink": {
									"path": "$['station_id']"
								}
							},
							{
								"source": {
									"path": "['station_status']"
								},
								"sink": {
									"path": "$['station_status']"
								}
							},
							{
								"source": {
									"path": "$['last_updated']"
								},
								"sink": {
									"path": "$['_ts']"
								}
							},
							{
								"source": {
									"path": "$['ttl']"
								},
								"sink": {
									"path": "$['ttl']"
								}
							}
						],
						"collectionReference": "$['data']['stations']"
					}
				},
				"inputs": [
					{
						"referenceName": "bike_status_src",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CosmosDbSqlApiCollection1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}