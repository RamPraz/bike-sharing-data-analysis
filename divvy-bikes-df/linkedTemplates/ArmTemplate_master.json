{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "divvy-bikes-df"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"CosmosDb1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDb1'"
		},
		"DivvyCosmosDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DivvyCosmosDB'"
		},
		"azradls_link_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azradls_link'"
		},
		"divvy-wh-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'divvy-wh-WorkspaceDefaultSqlServer'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azrbikesharing.dfs.core.windows.net"
		},
		"Divvy status api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gbfs.divvybikes.com/gbfs/fr/station_status.json"
		},
		"azradls_link_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azradls.dfs.core.windows.net"
		},
		"divvy-wh-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azradls.dfs.core.windows.net"
		},
		"station_status_api_call_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gbfs.divvybikes.com/gbfs/es/station_information.json"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "divvy-bikes-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"CosmosDb1_connectionString": {
						"value": "[parameters('CosmosDb1_connectionString')]"
					},
					"azradls_link_accountKey": {
						"value": "[parameters('azradls_link_accountKey')]"
					},
					"DivvyCosmosDB_connectionString": {
						"value": "[parameters('DivvyCosmosDB_connectionString')]"
					},
					"divvy-wh-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('divvy-wh-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"Divvy status api_properties_typeProperties_url": {
						"value": "[parameters('Divvy status api_properties_typeProperties_url')]"
					},
					"azradls_link_properties_typeProperties_url": {
						"value": "[parameters('azradls_link_properties_typeProperties_url')]"
					},
					"station_status_api_call_properties_typeProperties_url": {
						"value": "[parameters('station_status_api_call_properties_typeProperties_url')]"
					},
					"divvy-wh-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('divvy-wh-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}